<!DOCTYPE suppressions PUBLIC
    "-//Checkstyle//DTD Suppressions 1.2//EN"
    "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
  <!-- Suppress the JavadocPackage check for missing package-info.java -->
  <suppress checks="JavadocPackage" files=".*"/>
  <suppress checks="MissingJavadocMethod" files=".*"/>
  <suppress checks="HideUtilityClassConstructor" files=".*"/>
  <suppress checks="FinalParameters" files=".*"/>
  <suppress checks="JavadocVariable" files=".*" />
  <suppress checks="DesignForExtension" files=".*" />
  <suppress checks="HiddenField" files=".*" />
  <suppress checks="MissingJavadocType" files=".*" />
  <suppress checks="MissingJavadocMethod" files=".*" />
  <suppress checks="LineLength" files="src/test/.*" />
</suppressions>